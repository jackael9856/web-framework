<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:motan="http://api.weibo.com/schema/motan"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
       http://api.weibo.com/schema/motan http://api.weibo.com/schema/motan.xsd">

	<motan:referer id="roomService" interface="com.uxin.zb.room.service.RoomService"
		connectTimeout="3000" requestTimeout="30000" basicReferer="motantestClientBasicConfig" check="false" throwException="true"/>
	
	<motan:referer id="recommendService" interface="com.uxin.zb.room.service.RecommendService"
		connectTimeout="3000" requestTimeout="30000" basicReferer="motantestClientBasicConfig" check="false" throwException="true"/>
	
	<motan:referer id="timelineService" interface="com.uxin.zb.room.service.TimelineService"
		connectTimeout="3000" requestTimeout="3000" basicReferer="motantestClientBasicConfig" check="false" throwException="true"/>
		
	<motan:referer id="roomImageService" interface="com.uxin.zb.room.service.RoomImageService"
		connectTimeout="3000" requestTimeout="3000" basicReferer="motantestClientBasicConfig" check="false" throwException="true"/>
	
	<motan:referer id="roomCallBackService" interface="com.uxin.zb.room.service.RoomCallBackService"
		connectTimeout="3000" requestTimeout="3000" basicReferer="motantestClientBasicConfig" check="false" throwException="true"/>
		
	<motan:referer id="questionService" interface="com.uxin.zb.room.service.QuestionService"
		connectTimeout="1500" requestTimeout="1500" basicReferer="motantestClientBasicConfig" check="false" throwException="true"/>

	<motan:referer id="shareService" interface="com.uxin.zb.room.service.ShareService"
	    connectTimeout="1500" requestTimeout="1500" basicReferer="motantestClientBasicConfig" check="false" throwException="true"/>

</beans>
